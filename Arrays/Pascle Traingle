class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>>answer;
        for(int i=0;i<numRows;i++){
            vector<int>vec;
            int x=1;
            vec.push_back(x);
            for(int j=1;j<=i;j++){
                x=(x*(i-j+1))/j;
                vec.push_back(x);
            }
            answer.push_back(vec);  
        }
        return answer;
    }
};
